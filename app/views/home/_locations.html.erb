<h2>Your Saved Locations</h2>
  <table class="table table-striped table-bordered">
   <tr>
    <th>Lat</th>
    <th>Lng</th>
  </tr>
  <% @saved_locations.each do |loc| %>
  <tr>
    <td><%= loc.lat%></td>
    <td><%= loc.lng%></td>
    <td><%= link_to 'Remove', location_path(loc), :method => 'delete' %></td>
  </tr>
  <% end %> 
  </table>